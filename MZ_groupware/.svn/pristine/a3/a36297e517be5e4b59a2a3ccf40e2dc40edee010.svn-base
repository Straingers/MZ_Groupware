<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ page trimDirectiveWhitespaces="true" %>
<script>
Swal.fire({
  	position: 'top-end',
  	icon: "success",
  	title: "삭제가 완료되었습니다.",
  	buttonsStyling: false,
  	showConfirmButton: false,
  	timer: 1500
}).then((result) => {
	window.opener.parent.location.reload();
	window.close();
})
</script>