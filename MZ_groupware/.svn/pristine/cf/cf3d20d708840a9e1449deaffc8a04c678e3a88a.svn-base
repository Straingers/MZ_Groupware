package com.spring.command;

import java.util.Date;

import com.spring.dto.EmployeeVO;

public class EmployeeRegistCommand {
	private String empPhoto;
	private String qrCode;
	private String empNo;
	private String empName;
	private String empHp;
	private String empEmail;
	private String empRegno;
	private String empZipcode;
	private String empAddr1;
	private String empAddr2;
	private String deptCode;
	private String psCode;
	private Date empCreateDate;
	private String empTel;
	
	public String getEmpPhoto() {
		return empPhoto;
	}
	public void setEmpPhoto(String empPhoto) {
		this.empPhoto = empPhoto;
	}
	public String getQrCode() {
		return qrCode;
	}
	public void setQrCode(String qrCode) {
		this.qrCode = qrCode;
	}
	public String getEmpNo() {
		return empNo;
	}
	public void setEmpNo(String empNo) {
		this.empNo = empNo;
	}
	public String getEmpName() {
		return empName;
	}
	public void setEmpName(String empName) {
		this.empName = empName;
	}
	public String getEmpHp() {
		return empHp;
	}
	public void setEmpHp(String empHp) {
		this.empHp = empHp;
	}
	public String getEmpEmail() {
		return empEmail;
	}
	public void setEmpEmail(String empEmail) {
		this.empEmail = empEmail;
	}
	public String getEmpRegno() {
		return empRegno;
	}
	public void setEmpRegno(String empRegno) {
		this.empRegno = empRegno;
	}
	public String getEmpZipcode() {
		return empZipcode;
	}
	public void setEmpZipcode(String empZipcode) {
		this.empZipcode = empZipcode;
	}
	public String getEmpAddr1() {
		return empAddr1;
	}
	public void setEmpAddr1(String empAddr1) {
		this.empAddr1 = empAddr1;
	}
	public String getEmpAddr2() {
		return empAddr2;
	}
	public void setEmpAddr2(String empAddr2) {
		this.empAddr2 = empAddr2;
	}
	public String getDeptCode() {
		return deptCode;
	}
	public void setDeptCode(String deptCode) {
		this.deptCode = deptCode;
	}
	public String getPsCode() {
		return psCode;
	}
	public void setPsCode(String psCode) {
		this.psCode = psCode;
	}
	public Date getEmpCreateDate() {
		return empCreateDate;
	}
	public void setEmpCreateDate(Date empCreateDate) {
		this.empCreateDate = empCreateDate;
	}
	public String getEmpTel() {
		return empTel;
	}
	public void setEmpTel(String empTel) {
		this.empTel = empTel;
	}
	@Override
	public String toString() {
		return "EmployeeRegistCommand [empPhoto=" + empPhoto + ", qrCode=" + qrCode + ", empNo=" + empNo + ", empName="
				+ empName + ", empHp=" + empHp + ", empEmail=" + empEmail + ", empRegno=" + empRegno + ", empZipcode="
				+ empZipcode + ", empAddr1=" + empAddr1 + ", empAddr2=" + empAddr2 + ", deptCode=" + deptCode
				+ ", psCode=" + psCode + ", empCreateDate=" + empCreateDate + ", empTel=" + empTel + "]";
	}
	
	public EmployeeVO toEmployeeVO() {
		EmployeeVO employee = new EmployeeVO();
		employee.setEmpPhoto(empPhoto);
		employee.setQrCode(qrCode);
		employee.setEmpNo(empNo);
		employee.setEmpName(empName);
		employee.setEmpHp(empHp);
		employee.setEmpEmail(empEmail);
		employee.setEmpRegno(empRegno);
		employee.setEmpZipcode(empZipcode);
		employee.setEmpAddr1(empAddr1);
		employee.setEmpAddr2(empAddr2);
		employee.setDeptCode(deptCode);
		employee.setPsCode(psCode);
		employee.setEmpCreateDate(empCreateDate);
		employee.setEmpTel(empTel);
		
		return employee;
	}
}

