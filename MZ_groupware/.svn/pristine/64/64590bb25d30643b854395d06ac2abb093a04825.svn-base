package com.spring.msg.dto;

import com.spring.command.SearchCriteria;

public class MessageVO extends SearchCriteria{
	private int mssNo;
	private String empNo;
	private String sender;
	private String mssReceiver;
	private String receiver;
	private String mssContent;
	private String mssSenddate;
	private String mssOpenStatus;
	private String receiverDel;
	private String senderDel;
	private String pageType;
	private String openType;
	
	public String getOpenType() {
		return openType;
	}
	public void setOpenType(String openType) {
		this.openType = openType;
	}
	public String getPageType() {
		return pageType;
	}
	public void setPageType(String pageType) {
		this.pageType = pageType;
	}
	public String getReceiverDel() {
		return receiverDel;
	}
	public void setReceiverDel(String receiverDel) {
		this.receiverDel = receiverDel;
	}
	public String getSenderDel() {
		return senderDel;
	}
	public void setSenderDel(String senderDel) {
		this.senderDel = senderDel;
	}
	public int getMssNo() {
		return mssNo;
	}
	public void setMssNo(int mssNo) {
		this.mssNo = mssNo;
	}
	public String getSender() {
		return sender;
	}
	public void setSender(String sender) {
		this.sender = sender;
	}
	public String getReceiver() {
		return receiver;
	}
	public void setReceiver(String receiver) {
		this.receiver = receiver;
	}
	public String getEmpNo() {
		return empNo;
	}
	public void setEmpNo(String empNo) {
		this.empNo = empNo;
	}
	public String getMssReceiver() {
		return mssReceiver;
	}
	public void setMssReceiver(String mssReceiver) {
		this.mssReceiver = mssReceiver;
	}
	public String getMssContent() {
		return mssContent;
	}
	public void setMssContent(String mssContent) {
		this.mssContent = mssContent;
	}
	public String getMssSenddate() {
		return mssSenddate;
	}
	public void setMssSenddate(String mssSenddate) {
		this.mssSenddate = mssSenddate;
	}
	public String getMssOpenStatus() {
		return mssOpenStatus;
	}
	public void setMssOpenStatus(String mssOpenStatus) {
		this.mssOpenStatus = mssOpenStatus;
	}
	
	@Override
	public String toString() {
		return "MessageVO [mssNo=" + mssNo + ", empNo=" + empNo + ", sender=" + sender + ", mssReceiver=" + mssReceiver
				+ ", receiver=" + receiver + ", mssContent=" + mssContent + ", mssSenddate=" + mssSenddate
				+ ", mssOpenStatus=" + mssOpenStatus + ", receiverDel=" + receiverDel + ", senderDel=" + senderDel
				+ ", pageType=" + pageType + "]";
	}
}
